{
  "name": "actionware",
  "version": "7.0.0",
  "description": "Use Redux with much less boilerplate and get busy and error states for every action with no extra code.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "tsc&&cross-env NODE_PATH=./lib mocha ./tests/**/**/*.test.js --require before-tests.js --compilers js:babel-register",
    "test:watch": "cross-env NODE_PATH=./lib gulp test:watch",
    "build": "tsc&&babel lib -d lib",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wellguimaraes/actionware.git"
  },
  "keywords": [
    "redux",
    "actions",
    "boilerplate",
    "busy",
    "state",
    "error",
    "react"
  ],
  "author": "Wellington Guimaraes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wellguimaraes/actionware/issues"
  },
  "homepage": "https://github.com/wellguimaraes/actionware#readme",
  "peerDependencies": {
    "react": "^15.5.4",
    "redux": "^3.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "@types/sinon": "^2.3.1",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "cross-env": "^5.0.1",
    "enzyme": "^2.8.2",
    "gulp": "^3.9.1",
    "jsdom": "^9.0.0",
    "mocha": "^3.4.2",
    "mocha-jsdom": "^1.1.0",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "react-redux": "^5.0.5",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.7.0",
    "redux-mock-store": "^1.2.3",
    "sinon": "^2.3.4",
    "typescript": "^2.3.4",
    "@types/react": "^15.0.31",
    "gulp-mocha": "^4.3.1"
  }
}
